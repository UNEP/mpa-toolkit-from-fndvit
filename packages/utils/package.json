{
  "name": "@mpa/utils",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "types": "./utils.ts",
  "exports": {
    "import": "./dist/utils.js",
    "require": "./dist/utils.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf ./dist",
    "prepare": "pnpm build"
  },
  "tsup": {
    "entry": [
      "utils.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true
  }
}