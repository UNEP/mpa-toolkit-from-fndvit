{
  "name": "@mpa/utils",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "types": "./utils.ts",
  "exports": {
    ".": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf ./dist"
  },
  "tsup": {
    "entry": [
      "*.ts",
      "prisma/*.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ]
  },
  "dependencies": {
    "find-up": "^6.3.0",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.2",
    "shelljs": "^0.8.5"
  }
}