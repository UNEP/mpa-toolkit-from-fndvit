{
  "name": "@mpa/env",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "types": "./env.ts",
  "exports": {
    ".": {
      "import": "./dist/env.js",
      "require": "./dist/env.cjs"
    },
    "./load": {
      "import": "./dist/load.js",
      "require": "./dist/load.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@mpa/utils": "workspace:^1.0.0",
    "@types/node": "^18.7.5"
  },
  "tsup": {
    "entry": [
      "env.ts",
      "load.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ]
  }
}