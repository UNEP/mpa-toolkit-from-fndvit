{
  "name": "@mpa/events",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "types": "./events.ts",
  "exports": {
    "import": "./dist/events.js",
    "require": "./dist/events.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.145.0",
    "@mpa/env": "workspace:*",
    "@mpa/log": "workspace:*"
  },
  "tsup": {
    "entry": [
      "events.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ]
  }
}