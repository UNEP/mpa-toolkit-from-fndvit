{
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "prebuild": "yarn build:adapter && yarn clean",
    "build": "svelte-kit build",
    "presynth": "yarn build",
    "synth": "cdk synth --app ./adapter/build/stack.js \"*\" --require-approval never",
    "predeploy": "yarn build",
    "deploy": "cdk deploy --app ./adapter/build/stack.js \"*\" --require-approval never",
    "prebuild:adapter": "yarn clean:adapter",
    "build:adapter": "tsc -p adapter && cp -R adapter/lambda adapter/build/lambda",
    "clean:adapter": "rimraf adapter/build",
    "clean": "rimraf build cdk.out",
    "postinstall": "path-exists adapter/build || yarn build:adapter",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node --esm prisma/seed.ts",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",

    "test": "vitest"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.16.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.17.0-alpha.0",
    "@aws-sdk/client-s3": "^3.53.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/aws-lambda": "^8.10.93",
    "@types/cookie": "^0.4.1",
    "@types/d3-geo": "^3.0.2",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/js-cookie": "^3.0.1",
    "@types/jwt-decode": "^3.1.0",
    "@types/mime-types": "^2.1.1",
    "@types/pino-pretty": "^4.7.5",
    "@types/prosemirror-commands": "^1.0.4",
    "@types/prosemirror-model": "^1.16.0",
    "@types/prosemirror-view": "^1.23.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "aws-cdk-lib": "^2.16.0",
    "constructs": "^10.0.87",
    "esbuild": "^0.14.28",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "fs-extra": "^10.0.1",
    "path-exists-cli": "^2.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "prisma": "^3.10.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "svelte": "^3.46.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
    "vitest": "^0.10.2"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@prisma/client": "^3.12.0",
    "@splidejs/svelte-splide": "^0.1.18",
    "ajv": "^8.11.0",
    "clsx": "^1.1.1",
    "cookie": "^0.4.1",
    "d3-geo": "^3.0.1",
    "dotenv": "^16.0.0",
    "jwt-decode": "^3.1.2",
    "mime-types": "^2.1.34",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.3",
    "prosemirror-commands": "^1.2.1",
    "prosemirror-dropcursor": "^1.4.0",
    "prosemirror-gapcursor": "^1.2.1",
    "prosemirror-history": "^1.2.0",
    "prosemirror-inputrules": "^1.1.3",
    "prosemirror-keymap": "^1.1.5",
    "prosemirror-model": "^1.16.1",
    "prosemirror-schema-list": "^1.1.6",
    "prosemirror-state": "^1.3.4",
    "prosemirror-view": "^1.23.6",
    "sk-auth": "^0.4.1",
    "svelte-modals": "^1.1.0",
    "svelte-multiselect": "^4.0.1",
    "svelte-scrollto": "^0.2.0",
    "topojson": "^3.0.2",
    "uuid": "^8.3.2"
  }
}
